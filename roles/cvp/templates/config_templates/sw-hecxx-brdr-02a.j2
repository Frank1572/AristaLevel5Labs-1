hostname sw-hec{{ hec.hec_nr }}-brdr-02a
!
interface Ethernet1
   description "->rt-hec{{ hec.hec_nr }}-vpn-01a, Te0/1/1"
interface Ethernet2
   description "->rt-hec{{ hec.hec_nr }}-vpn-01b, Te0/1/1"
interface Ethernet3
   description "->lb-hec{{ hec.hec_nr }}-01, e1.1"
interface Ethernet4
   description "->lb-hec{{ hec.hec_nr }}-01, e1.2"
interface Ethernet5
   description "->lb-hec{{ hec.hec_nr }}-01, e1.3"
interface Ethernet6
   description "->fw-hec{{ hec.hec_nr }}-01, et1-01"
interface Ethernet7
   description "->fw-hec{{ hec.hec_nr }}-02, et1-02"
!
interface Ethernet47
   description ->fwm-hec-emea-09, eth1-01
   channel-group 30 mode active
!
interface Ethernet48
   description "->naa-hec{{ hec.hec_nr }}-01, e3"
   switchport access vlan 916
   channel-group 48 mode active
!
interface Ethernet51/1
   ip address 10.11.0.5/31
interface Ethernet52/1
   ip address 10.12.0.5/31
interface Ethernet53/1
   ip address 10.13.0.5/31
interface Ethernet54/1
   ip address 10.14.0.5/31
!
interface Loopback1
   description Router-ID
   ip address 10.100.1.2/32
!
interface Loopback900
   description NW-MGMT
   ip address {{ hec.lo900_prefix }}.17/32
!
interface Management1
   vrf forwarding OOB
   ip address {{ hec.oob_prefix }}.17/{{ hec.oob_mask }}
!
interface Vlan916
   ip address {{ hec.inband_trans_prefix }}.76/{{ hec.inband_trans_mask }}
!
router bgp 641{{ hec.hec_nr }}.10002
   router-id 10.100.1.2
   neighbor 10.11.0.4 peer-group SPINE-PG
   neighbor 10.12.0.4 peer-group SPINE-PG
   neighbor 10.13.0.4 peer-group SPINE-PG
   neighbor 10.14.0.4 peer-group SPINE-PG
   neighbor 198.19.224.2 peer-group MLPEER-PG
   network 10.100.1.2/32
   network {{ hec.lo900_prefix }}.17/32


