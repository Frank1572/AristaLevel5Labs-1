peer-filter SUPERSPINE-PF
   10 match as-range 4201064208 result accept
   20 match as-range 4201074208 result accept
   30 match as-range 4201084208 result accept
   40 match as-range 4201094208 result accept
   50 match as-range 4201104208 result accept
   60 match as-range 4201114208 result accept
!
router bfd
   slow-timer 5000
   interval 200 min-rx 200 multiplier 3 default
   multihop interval 1000 min-rx 1000 multiplier 5
!
router bgp 64103.9000
  router-id 10.90.3.1
  bgp asn notation asdot
  no bgp default ipv4-unicast
  distance bgp 20 200 200
  maximum-paths 16 ecmp 16
  bgp listen range 10.100.0.0/24 peer-group EVPN-OVERLAY-DYNAMIC peer-filter SUPERSPINE-PF
  bgp listen range 10.110.3.0/24 peer-group EVPN-OVERLAY-DYNAMIC peer-filter SUPERSPINE-PF
  bgp listen range 10.120.3.0/24 peer-group EVPN-OVERLAY-DYNAMIC peer-filter SUPERSPINE-PF
  bgp listen range 10.130.3.0/24 peer-group EVPN-OVERLAY-DYNAMIC peer-filter SUPERSPINE-PF
  bgp listen range 10.140.3.0/24 peer-group EVPN-OVERLAY-DYNAMIC peer-filter SUPERSPINE-PF
  bgp listen range 10.150.3.0/24 peer-group EVPN-OVERLAY-DYNAMIC peer-filter SUPERSPINE-PF
  bgp listen range 10.255.0.0/16 peer-group UNDERLAY-DYNAMIC peer-filter SUPERSPINE-PF
  neighbor EVPN-OVERLAY-DYNAMIC peer group
  neighbor EVPN-OVERLAY-DYNAMIC update-source Loopback1
  neighbor EVPN-OVERLAY-DYNAMIC bfd
  neighbor EVPN-OVERLAY-DYNAMIC ebgp-multihop
  neighbor EVPN-OVERLAY-DYNAMIC send-community standard extended
  neighbor EVPN-OVERLAY-DYNAMIC maximum-routes 0
  neighbor UNDERLAY-DYNAMIC peer group
  neighbor UNDERLAY-DYNAMIC send-community standard extended
  neighbor UNDERLAY-DYNAMIC maximum-routes 12000
  neighbor UNDERLAY-DYNAMIC rib-in pre-policy retain all
  address-family evpn
    bgp next-hop-unchanged
    neighbor EVPN-OVERLAY-DYNAMIC activate
    no neighbor UNDERLAY-DYNAMIC activate
  address-family ipv4
    no neighbor EVPN-OVERLAY-DYNAMIC activate
    neighbor UNDERLAY-DYNAMIC activate
  address-family ipv4
    network 10.90.3.1/32
    network 198.19.156.65/32
