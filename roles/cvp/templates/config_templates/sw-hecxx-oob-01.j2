mlag configuration
   domain-id MLAG-oob
!
vlan 4094
   name MLAG_Vlan
!
alias dhcp-off
   10 bash sudo service dhcpd stop
   20 bash sudo service dhcpd status
!
alias dhcp-on
   10 bash sudo cp /mnt/flash/dhcpd.conf /etc/dhcp/
   20 bash sudo service dhcpd start
   30 bash sudo service dhcpd status
!
alias dhcp-status
   10 bash sudo service dhcpd status
   20 bash sudo cat /var/lib/dhcpd/dhcpd.leases
!
ip radius source-interface Vlan21
!
logging source-interface Vlan21
!
ntp source Vlan21
!
interface Port-Channel102
   description ->sw-hec{{ hec.hec_nr }}-oob-02,Po101
   port-channel lacp fallback individual
   switchport trunk allowed vlan 21
   switchport mode trunk
   mlag 102
!
interface Port-Channel103
   description ->sw-hec{{ hec.hec_nr }}-oob-03,Po101
   port-channel lacp fallback individual
   switchport trunk allowed vlan 21
   switchport mode trunk
   mlag 103
!
interface Port-Channel104
   description ->sw-hec{{ hec.hec_nr }}-oob-04,Po101
   port-channel lacp fallback individual
   switchport trunk allowed vlan 21
   switchport mode trunk
   mlag 104
!
interface Port-Channel105
   description ->sw-hec{{ hec.hec_nr }}-oob-05,Po101
   port-channel lacp fallback individual
   switchport trunk allowed vlan 21
   switchport mode trunk
   mlag 105
!
interface Port-Channel106
   description ->sw-hec{{ hec.hec_nr }}-oob-06,Po101
   port-channel lacp fallback individual
   switchport trunk allowed vlan 21
   switchport mode trunk
   mlag 106
!
interface Port-Channel107
   description ->sw-hec{{ hec.hec_nr }}-oob-07,Po101
   port-channel lacp fallback individual
   switchport trunk allowed vlan 21
   switchport mode trunk
   mlag 107
!
interface Ethernet1-46
   switchport access vlan 21
!
interface Ethernet1
   description ->naa-hec{{ hec.hec_nr }}-01 Mgm1
!
interface Ethernet47-48
   description ->sw-hec{{ hec.hec_nr }}-oob-01-mlag-peer
   channel-group 42 mode active
!
interface Ethernet21
   channel-group 102 mode active
!
interface Ethernet22
   channel-group 103 mode active
!
interface Ethernet23
   channel-group 104 mode active
!
interface Ethernet24
   channel-group 105 mode active
!
interface Ethernet25
   channel-group 106 mode active
!
interface Ethernet26
   channel-group 107 mode active
!
interface Ethernet49
   no switchport
!
interface Loopback494
   description !!! DO NOT TOUCH - HSRP tracking interface !!!
!
track 494 interface Loopback494 line-protocol
!
interface Vlan21
   vrrp 21 preempt delay minimum 180
   vrrp 21 preempt delay reload 180
   vrrp 21 authentication text oobnw
   vrrp 21 ip {{ hec.oob_def_gw }}
   vrrp 21 track 494 decrement 10
!
ip route {{ hec.inband_trans_cvp }}/32 {{ hec.oob_cvp }}
!
ip routing
!
ip prefix-list MATCH_ANY seq 5 permit 0.0.0.0/0 le 32
ip prefix-list PREF_ADMIN_MPLS seq 5 permit {{ hec.admin_mpls_1 }}
ip prefix-list PREF_ADMIN_MPLS seq 10 permit {{ hec.admin_mpls_2 }}
ip prefix-list PREF_ADMIN_MPLS seq 15 permit {{ hec.admin_mpls_3 }}
ip prefix-list PREF_ADMIN_MPLS seq 20 permit {{ hec.admin_mpls_4 }}
!
route-map ADMIN_MPLS_IN deny 10
   match ip address prefix-list PREF_ADMIN_MPLS
!
route-map ADMIN_MPLS_IN permit 20
   match ip address prefix-list MATCH_ANY
!
route-map ADMIN_MPLS_OUT permit 10
   match ip address prefix-list PREF_ADMIN_MPLS
!
router ospf 1
   passive-interface default
   no passive-interface {{ hec.admin_sw_interface }}
   no passive-interface Vlan4094
   network {{ hec.inband_trans_prefix }}.0/24 area 0.0.0.0
   network 198.19.224.0/30 area 0.0.0.0
   network {{ hec.oob01_lo0_prefix }}.0/24 area 0.0.0.0
   max-lsa 12000
!
interface Vlan4094
   ip ospf network point-to-point
!

