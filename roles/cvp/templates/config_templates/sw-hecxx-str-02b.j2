hostname sw-hec{{ hec.hec_nr }}-str-02b
!
interface Ethernet51/1
   ip address 10.11.0.19/31
interface Ethernet52/1
   ip address 10.12.0.19/31
interface Ethernet53/1
   ip address 10.13.0.19/31
interface Ethernet54/1
   ip address 10.14.0.19/31
!
interface Loopback1
   description Router-ID
   ip address 10.100.1.105/32
interface Loopback900
   description NW-MGMT
   ip address {{ hec.lo900_prefix }}.54/32
interface Management1
   vrf forwarding OOB
   ip address {{ hec.oob_prefix }}.54/{{ hec.oob_mask }}
!
router bgp 641{{ hec.hec_nr }}.10005
   router-id 10.100.1.105
   neighbor 10.11.0.18 peer-group SPINE-PG
   neighbor 10.12.0.18 peer-group SPINE-PG
   neighbor 10.13.0.18 peer-group SPINE-PG
   neighbor 10.14.0.18 peer-group SPINE-PG
   neighbor 198.19.224.1 peer-group MLPEER-PG
   network 10.100.1.105/32
   network {{ hec.lo900_prefix }}.54/32
!
